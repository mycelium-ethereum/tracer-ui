{
  "name": "@tracer-protocol/tracer-ui",
  "version": "1.0.4",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "description": "A skeleton to create your own React component library using Rollup, TypeScript, Sass and Storybook",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "check-format": "eslint src/**/*.{ts,tsx} && prettier --check 'src/**/*.{ts,tsx}'",
    "generate-atom": "node ./util/create-component atoms",
    "generate-molecule": "node ./util/create-component molecules",
    "generate-organism": "node ./util/create-component organisms",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "test-build": "yarn build && node ./util/copy-build"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Storybook"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@storybook/theming": "^6.4.12",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "chromatic": "^6.3.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-storybook": "^0.5.7",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "2.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^1.27.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "storybook-dark-mode": "^1.0.8",
    "styled-components": "^5.1.1",
    "ts-jest": "^24.2.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@popperjs/core": "^2.11.2",
    "@svgr/rollup": "^6.2.0",
    "@svgr/webpack": "5.1.0",
    "react-modal": "^3.14.4",
    "react-popper": "^2.2.5",
    "react-popper-2": "npm:react-popper@^2.2.5",
    "rollup-plugin-import-css": "^3.0.2"
  }
}
